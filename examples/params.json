{
    // Solver
    // -----------------------------------------------------------------------
    // method   integration method to use for solver
    // rtol     relative tolerance for solver
    // tf       final time for solver [s]
    "method": "Radau",
    "rtol": 1e-4,
    "tf": 1000,

    // Grid points
    // -----------------------------------------------------------------------
    // N1       below fuel inlet in bed
    // N2       above fuel inlet in bed
    // N3       in freeboard
    "N1": 50,
    "N2": 25,
    "N3": 25,

    // Initial conditions
    // -----------------------------------------------------------------------
    // Ts0      initial solid temperature [K]
    // Tg0      initial gas temperature [K]
    // rhobb0   initial biomass concentration [kg/m³]
    // mfg0     initial gas mass flux [kg/(s⋅m²)]
    // rhobg0   initial bulk gas concentration [kg/m³]
    // rhobh2o0 initial H₂O concentration [kg/m³]
    // Tp0      initial inert bed temperature [K]
    // rhobc0   initial char concentration [kg/m³]
    // rhobh20  initial H₂ concentration [kg/m³]
    // rhobch40 initial CH₄ concentration [kg/m³]
    // rhobco0  initial CO concentration [kg/m³]
    // rhobco20 initial CO₂ concentration [kg/m³]
    // rhobt0   initial tar concentration [kg/m³]
    // rhobca0  initial accumulated char concentration [kg/m³]
    // Tw0      initial wall temperature [K]
    "Ts0": 300,
    "Tg0": 1100,
    "rhobb0": 1e-12,
    "mfg0": 0.2,
    "rhobg0": 0.15,
    "rhobh2o0": 0.15,
    "Tp0": 1100,
    "rhobc0": 0,
    "rhobh20": 1e-12,
    "rhobch40": 1e-12,
    "rhobco0": 1e-12,
    "rhobco20": 1e-12,
    "rhobt0": 1e-12,
    "rhobca0": 1e-12,
    "Tw0": 1100,

    // Reactor
    // -----------------------------------------------------------------------
    // Db       internal diameter of the bed [m]
    // Dwo      external diameter of the reactor [m]
    // L        overall reactor height [m]
    // Lb       biomass feeding position above distributor [m]
    // Ls       height of fuel inlet above distributor [m]
    // Lsi      total initial bed height above distributor [m]
    // Lu       gas feeding position above distributor [m]
    // Uha      overall heat transfer coefficient between wall and ambient [W/(m²⋅K)]
    // cpw      specific heat capacity of steel [J/kg K]
    // ew       reactor wall emissivity [-]
    // kw       thermal conductivity of the reactor steel wall [W/(m⋅K)]
    // rhow     density of the reactor steel wall [kg/m³]
    // xw       thickness of the reactor wall [m]
    "Db": 0.1,
    "Dwo": 0.1042,
    "L": 1.0,
    "Lb": 0.212,
    "Ls": 0.212,
    "Lsi": 0.22,
    "Lu": 0,
    "Uha": 0.018,
    "cpw": 502,
    "ew": 0.13,
    "kw": 16,
    "rhow": 7930,
    "xw": 0.00211,

    // Gas phase
    // -----------------------------------------------------------------------
    // Mgin     molecular weight at gas inlet [g/mol]
    // P        inlet gas pressure [Pa], this is not required
    // Pa       ambient pressure [Pa]
    // SB       steam to biomass ratio [-]
    // Tam      ambient temperature [K]
    // Tgin     gas inlet temperature [K]
    "Mgin": 18,
    "P": 1.013e5,
    "Pa": 1.013e5,
    "SB": 1.0,
    "Tam": 300,
    "Tgin": 1073,

    // Solid phase
    // -----------------------------------------------------------------------
    // Gp       bulk modulus (elasticity property) of inert particles [GPa]
    // Gs       bulk modulus (elasticity property) of fuel (wood) particles [GPa]
    // Tsin     solid fuel inlet temperature [K]
    // cf       Coulomb coefficient of friction between wood and sand [-]
    // cpp      specific heat capacity of inert particle [J/(kg⋅K)]
    // db0      diameter of raw biomass particle (assumed cylindrical) [m]
    // dp       diameter of the inert bed particle [m]
    // e        coefficient of restitution between wood and sand [-]
    // ef0      initial void fraction of the inert bed particles [-]
    // emf      void fraction of the bed of inert particles at minimum fluidization [-]
    // ep       emissivity of inert particles [-]
    // es       emissivity of fuel (wood) particles [-]
    // gamp     Poisson's ratio (elasticity property) of inert particles [-]
    // gams     Poisson's ratio (elasticity property) of fuel (wood) particles [-]
    // kp       thermal conductivity of inert particles [W/(m⋅K)]
    // ks       thermal conductivity of fuel particles (assumed same for biomass and char) [W/(m⋅K)]
    // lb       length of raw biomass particle (assumed cylindrical) [m]
    // msdot    mass flow rate of biomass [kg/hr]
    // n1       fragmentation factor of biomass particles [-]
    // phi      sphericity of inert bed particle [-]
    // rhob     density of biomass particle [kg/m³]
    // rhoc     density of char particle [kg/m³]
    // rhop     density of inert bed particle [kg/m³]
    // wH2O     weight fraction of moisture in the raw biomass particle [-]
    // wa       weight fraction of ash in the raw biomass particle [-]
    // wc       weight fraction of char in the raw biomass particle [-]
    "Gp": 50.0,
    "Gs":36.5,
    "Tsin": 300,
    "cf": 0.62,
    "cpp": 830,
    "db0": 6.0e-3,
    "dp": 6.15e-4,
    "e": 0.9,
    "ef0": 0.454,
    "emf": 0.454,
    "ep": 0.76,
    "es":0.95,
    "gamp": 0.25,
    "gams": 0.425,
    "kp": 0.260,
    "ks": 0.25,
    "lb": 1.33e-2,
    "msdot": 2.0,
    "n1": 1.4,
    "phi": 0.72,
    "rhob": 423,
    "rhoc": 150,
    "rhop": 2650,
    "wH2O": 0.062,
    "wa": 0.0041,
    "wc": 0.144
}
